{
  "expo": {
    "name": "Firebase Login Template",
    "slug": "firebase-login-template",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "../assetss/icon.png",
    "splash": {
      "image": "../assetss/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "../assetss/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "../assetss/favicon.png"
    }
  }
}
